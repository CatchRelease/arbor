{
  "name": "@catchandrelease/arbor",
  "version": "0.97.9",
  "description": "React component library for Catch&Release",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "TZ=UTC jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "yarn eslint && stylelint src",
    "eslint": "eslint src stories .storybook",
    "test:ci": "yarn typecheck && yarn test && yarn lint",
    "deploy-storybook": "yarn storybook-to-ghpages",
    "build": "yarn typecheck && yarn build:clean && yarn build:ts && yarn build:js",
    "build:clean": "rm -rf dist/*",
    "build:js": "babel src -d dist --extensions \".js,.ts,.tsx\" --ignore '**/*.test.js'",
    "build:ts": "tsc --emitDeclarationOnly --noEmit false",
    "typecheck": "tsc",
    "prepare": "husky install"
  },
  "author": "Damian Galarza",
  "contributors": [
    "Anthony Costanzo",
    "Euan Lau"
  ],
  "license": "MIT",
  "repository": "git@github.com:CatchRelease/arbor.git",
  "engines": {
    "node": ">=8.0.0 <=14.2"
  },
  "dependencies": {
